rn;\n            }\n\n            if (checkDraw()) {\n                gameActive = false;\n                statusDisplay.textContent = 'å¹³å±€ï¼ğŸ¤';\n                return;\n            }\n\n            currentPlayer = currentPlayer === 'X' ? 'O' : 'X';\n            statusDisplay.textContent = `ç©å®¶ ${currentPlayer} çš„å›åˆ`;\n        }\n\n        // ç”µè„‘ç§»åŠ¨ï¼ˆç®€å• AIï¼‰\n        function computerMove() {\n            if (!gameActive) return;\n\n            let move = findBestMove();\n            makeMove(move);\n        }\n\n        // å¯»æ‰¾æœ€ä½³ç§»åŠ¨ï¼ˆç®€å•ç­–ç•¥ï¼‰\n        function findBestMove() {\n            // 1. æ£€æŸ¥æ˜¯å¦å¯ä»¥è·èƒœ\n            let move = findWinningMove('O');\n            if (move !== -1) return move;\n\n            // 2. é˜»æ­¢ç©å®¶è·èƒœ\n            move = findWinningMove('X');\n            if (move !== -1) return move;\n\n            // 3. å æ®ä¸­å¿ƒ\n            if (board[4] === '') return 4;\n\n            // 4. å æ®è§’è½\n            const corners = [0, 2, 6, 8];\n            const emptyCorners = corners.filter(i => board[i] === '');\n            if (emptyCorners.length > 0) {\n                return emptyCorners[Math.floor(Math.random() * emptyCorners.length)];\n            }\n\n            // 5. å æ®è¾¹\n            const edges = [1, 3, 5, 7];\n            const emptyEdges = edges.filter(i => board[i] === '');\n            if (emptyEdges.length > 0) {\n                return emptyEdges[Math.floor(Math.random() * emptyEdges.length)];\n            }\n\n            return -1;\n        }\n\n        // å¯»æ‰¾è·èƒœ/é˜»æ­¢çš„ç§»åŠ¨\n        function findWinningMove(player) {\n            for (let combination of winningCombinations) {\n                const [a, b, c] = combination;\n                const cells = [board[a], board[b], board[c]];\n                const playerCount = cells.filter(cell => cell === player).length;\n                const emptyCount = cells.filter(cell => cell === '').length;\n\n                if (playerCount === 2 && emptyCount === 1) {\n                    if (board[a] === '') return a;\n                    if (board[b] === '') return b;\n                    if (board[c] === '') return c;\n                }\n            }\n            return -1;\n        }\n\n        // æ£€æŸ¥è·èƒœ\n        function checkWin() {\n            return winningCombinations.some(combination => {\n                return combination.every(index => board[index] === currentPlayer);\n            });\n        }\n\n        // è·å–è·èƒœç»„åˆ\n        function getWinningCombination() {\n            for (let combination of winningCombinations) {\n                if (combination.every(index => board[index] === currentPlayer)) {\n                    return combination;\n                }\n            }\n            return null;\n        }\n\n        // é«˜äº®è·èƒœæ ¼å­\n        function highlightWinningCells() {\n            const winningCombo = getWinningCombination();\n            if (winningCombo) {\n                winningCombo.forEach(index => {\n                    cells[index].classList.add('winner');\n                });\n            }\n        }\n\n        // æ£€æŸ¥å¹³å±€\n        function checkDraw() {\n            return board.every(cell => cell !== '');\n        }\n\n        // é‡æ–°å¼€å§‹æ¸¸æˆ\n        function restartGame() {\n            board = ['', '', '', '', '', '', '', '', ''];\n            currentPlayer = 'X';\n            gameActive = true;\n\n            cells.forEach(cell => {\n                cell.textContent = '';\n                cell.classList.remove('taken', 'x', 'o', 'winner');\n            });\n\n            statusDisplay.textContent = `ç©å®¶ ${currentPlayer} çš„å›åˆ`;\n        }\n\n        // è®¾ç½®æ¸¸æˆæ¨¡å¼\n        function setGameMode(isVsComputer) {\n            vsComputer = isVsComputer;\n            pvpBtn.classList.toggle('active', !vsComputer);\n            pvcBtn.classList.toggle('active', vsComputer);\n            restartGame();\n        }\n\n        // æ›´æ–°åˆ†æ•°æ˜¾ç¤º\n        function updateScoreDisplay() {\n            xScoreDisplay.textContent = scores.X;\n            oScoreDisplay.textContent = scores.O;\n        }\n\n        // å¯åŠ¨æ¸¸æˆ\n        initGame();\n    </script>\n</body>\n</html>\n"
